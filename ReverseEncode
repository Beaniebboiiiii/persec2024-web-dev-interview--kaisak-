function reverseString(str: string): string {
    let result = "";
    for (let i = str.length - 1; i >= 0; i--) {
        result += str[i];
    }
    return result;
}
const givenStr = "hello world";
const reversedStr = reverseString(givenStr);
console.log("dlrow olleh:", givenStr);
console.log("hello world:", reversedStr);
